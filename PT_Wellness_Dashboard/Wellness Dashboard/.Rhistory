rename_list <- c(start     = names(data)[2],
end       = names(data)[3],
sane      = names(data)[4],
wellness  = names(data)[5],
severity  = names(data)[6],
frequency = names(data)[7],
sleep     = names(data)[8],
name      = names(data)[9])
data <- data %>%
rename(all_of(rename_list))
data$Name  <- NULL
data$Email <- NULL
data$Last_modified_time <- NULL
data <- data %>%
mutate(
time = end - start
)
data
amount_changed <- function(x) {
change <- mean(tail(x, 3), na.rm = TRUE) - head(x, 1)
if (change >= 0) {
paste("+", change, sep = "")
} else {
paste("-", change, sep = "")
}
#round((tail(x, 1) - head(x, 1)) / head(x, 1) * 100, digits = 1)
}
amount_changed(data$sane)
